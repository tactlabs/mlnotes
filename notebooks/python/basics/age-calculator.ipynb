    "from datetime import datetime"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "346a1105-4801-4921-a8f5-f7168192082c",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_age(d):\n",
    "    d1 = datetime.now()\n",
    "    months = (d1.year - d.year) * 12 + d1.month - d.month\n",
    "\n",
    "    year = int(months / 12)\n",
    "    return year"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "cdebb4c5-1327-4806-b92b-6dc9f125c2e6",
   "metadata": {},
   "outputs": [],
   "source": [
    "age = get_age(datetime(1991, 1, 1))"
   ]
  },
